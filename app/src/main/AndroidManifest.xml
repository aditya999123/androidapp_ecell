<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="app.startups.nitrr.ecell.ecellapp">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/MyMaterialTheme">
        <activity android:name=".splash_screen.view.SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <activity android:name=".home.view.Home" />
        <activity android:name=".welcome.Welcome" />
        <activity android:name=".events.Events" />
        <activity android:name=".about_us.AboutUs" />
        <activity android:name=".blogs.view.Blogs"
            android:label="@string/activity_blogs"
            />
        <activity android:name=".contact_us.ContactUs" />
        <activity android:name=".Login.Login" />
        <activity android:name=".SignUp.SignUp" />

        <activity android:name=".sign_in.view.SignInActivity">

        </activity>

    </application>

</manifest>
